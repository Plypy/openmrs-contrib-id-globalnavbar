<% if (prefs.customCSS) {
	%><style><%-prefs.customCSS%></style><%
} %>
<div id="globalnav">
	<% if (prefs.cseId) { %><form method="get" action="https://www.google.com/cse">
        <input type="hidden" name="cx" value="<%= prefs.cseId %>">
        <input type="hidden" name="ie" value="UTF-8">
        <input type="text" class="field" name="q" id="s" placeholder="<%= prefs.csePlaceholder %>">
    </form><% } %>
    <% for (i in links) { %>
    <a target="_parent"<% if (matches == links[i]) { %> href="" class="yourhere"<% } else { %> href="<%= links[i].url %>"<% } %>><%= links[i].name %></a>
    <% } %>
    <a id="globalnav-state-button">[hide]</a>
</div>
